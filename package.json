{
  "name": "paste-as-string-array",
  "displayName": "Paste as String Array",
  "description": "Paste text as string array",
  "publisher": "shadyboukhary",
  "author": {
    "name": "Shady Boukhary",
    "email": "sb199898.sb@gmail.com"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:paste-as-string-array.pasteAsStringArray"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "paste-as-string-array.pasteAsStringArray",
        "title": "Paste as String Array"
      }
    ],
    "keybindings":[
      {
        "command": "paste-as-string-array.pasteAsStringArray",
        "key": "ctrl+alt+v",
        "mac": "shift+cmd+v",
        "linux": "ctrl+alt+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
